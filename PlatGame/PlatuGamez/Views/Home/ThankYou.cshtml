        .overlay {
            position: fixed;
            top: 0;
            bottom: 0;
            left: 0;
            right: 0;
            background: rgba(0,0,0,.7);
            visibility: hidden;
            opacity: 0;
            z-index: 3
        }

            .overlay:target {
                visibility: visible;
                opacity: 1
            }

        .popup {
            margin: 70px auto;
            padding: 20px;
            background: #fff;
            border-radius: 5px;
            width: 80%;
            position: relative;
            transition: all 1s ease-in-out !important;
            max-height: calc(100vh - 140px)
        }

            .popup h2 {
                margin-top: 0;
                color: #333;
                font-family: "Roboto", sans-serif;
            }

            .popup .close {
                position: absolute;
                top: 3px;
                right: 12px;
                transition: all .2s;
                font-size: 30px;
                font-weight: 700;
                text-decoration: none;
                color: #333;
            }

                .popup .close:hover {
                    color: #06d85f;
                }

            .popup .content {
                position: relative;
                overflow: auto;
                text-align: justify;
            }

        #privacy-popup .popup .content, #tnc-popup .popup .content {
            height: calc(100vh - 220px)
        }

        .popup .content .content-container {
            padding-right: 20px;
            padding-bottom: 20px;
            left: 0;
            top: 0;
            width: 100%;
            height: 100%;
            overflow-y: auto
        }

            .popup .content .content-container .faq-container ol {
                list-style-type: lower-alpha;
                padding-right: 10%
            }

        .popup .content ol {
            list-style-type: decimal;
            margin: 10px 0 0
        }

        .content ol {
            font-size: 16px;
        }

        li {
            line-height: 1.1;
        }

        .content p {
            font-size: .9rem;
            line-height: 1.5rem;
        }

        .content ol {
            display: block;
            list-style-type: upper-roman;
            margin-block-start: 0.2em;
            margin-block-end: 0.5em;
            margin-inline-start: 0px;
            margin-inline-end: 0px;
            /*padding-inline-start: 16px;*/
        }

            .content ol ol {
                padding-inline-start: 14px;
            }

        .content dd {
            padding-inline-start: 0;
            font-size: 0.8rem;
            font-weight: normal;
            text-transform: unset;
            margin-bottom: 5px;
        }

        .content ol li {
            font-size: 1rem;
            line-height: 1.5rem;
            font-weight: 600;
            text-transform: uppercase;
        }

        .content ol ol li {
            font-size: 0.9rem;
            font-weight: normal;
            list-style-type: none;
            text-transform: unset;
        }

        .content ol ol ol li {
            font-size: 0.9rem;
            font-weight: normal;
            list-style-type: lower-latin;
            text-transform: unset;
        }

        .content ol ol ol ol li {
            font-size: 0.9rem;
            font-weight: normal;
            list-style-type: lower-roman;
            text-transform: unset;
        }

        .content .privacy-list .privacy-item {
            line-height: 1.5rem;
            list-style-item: unset;
            text-transform: unset;
        }

        @@media screen and (max-width:700px) {
            .overlay {
                padding: 0 10px
            }

            .box {
                width: 95%
            }

            .popup {
                /* width: 100%; */
                margin: 20px auto;
                max-height: calc(100vh - 60px);
                padding: 40px 20px 20px
            }

            #privacy-popup .popup .content .content-container, #tnc-popup .popup .content .content-container {
                position: unset
            }

            #privacy-popup .popup .content, #tnc-popup .popup .content {
                position: relative;
                height: 100%
            }

            .popup .close {
                top: 10px;
                right: 15px
            }

            .popup h2 {
                font-size: 1rem;
            }

            .faq-container h2 {
                font-size: .7rem;
            }

            #privacy-popup li, #tnc-popup li, #tnc-popup p, .faq-container details p {
                line-height: .9rem;
                font-size: .7rem;
                margin-bottom: 0
            }

            #privacy-popup .popup .content, #tnc-popup .popup .content {
                height: calc(100vh - 280px)
            }
        }
        /* end modal styling */
    </style>
